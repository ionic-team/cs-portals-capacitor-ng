{
	"appId": "io.ionic.starter",
	"appName": "Portals Ecommerce",
	"webDir": "../../dist/apps/shell",
	"bundledWebRuntime": false,
	"plugins": {
		"FederatedCapacitor": {
			"shell": {
				"name": "shell"
			},
			"apps": [
				{
					"name": "account",
					"webDir": "../../dist/apps/account"
				},
				{
					"name": "checkout",
					"webDir": "../../dist/apps/checkout"
				},
				{
					"name": "help",
					"webDir": "../../dist/apps/help",
					"liveUpdateConfig": {
						"appId": "44897b9b",
						"channel": "production",
						"autoUpdateMethod": "background"
					}
				}
			]
		}
	}
}
